{
	"name": "nest-project",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "nest build",
		"dev": "concurrently \"cross-env NODE_ENV=development nest start --watch\" \"npm run prisma:watch\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prebuild": "rimraf dist",
		"prisma:format": "prisma-case-format -f prisma/schema.prisma",
		"prisma:generate": "prisma generate",
		"prisma:migrate:deploy": "prisma migrate deploy",
		"prisma:migrate:new": "prisma migrate dev --create-only",
		"prisma:watch": "nodemon --watch prisma/schema.prisma --exec npm run prisma:generate",
		"start:debug": "nest start --debug --watch",
		"start:prod": "cross-env NODE_ENV=production node dist/main",
		"test": "jest",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:watch": "jest --watch"
	},
	"dependencies": {
		"@nestjs/common": "^11.1.9",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.9",
		"@nestjs/jwt": "^11.0.1",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.1.9",
		"@nestjs/schedule": "^6.0.1",
		"@nestjs/swagger": "^11.2.3",
		"@prisma/client": "^6.19.0",
		"bcrypt": "^6.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"cookie-parser": "^1.4.7",
		"cross-env": "^10.1.0",
		"joi": "^18.0.2",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"pg": "^8.16.3",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.1.2",
		"rxjs": "^7.8.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@nestjs/cli": "^11.0.12",
		"@nestjs/schematics": "^11.0.9",
		"@nestjs/testing": "^11.1.9",
		"@types/bcrypt": "^6.0.0",
		"@types/compression": "^1.8.1",
		"@types/cookie-parser": "^1.4.10",
		"@types/express": "^5.0.5",
		"@types/jest": "^30.0.0",
		"@types/node": "^22.19.1",
		"@types/passport-jwt": "^4.0.1",
		"@types/passport-local": "^1.0.38",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^8.47.0",
		"concurrently": "^9.2.1",
		"eslint": "^9.39.1",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-boundaries": "^5.3.0",
		"eslint-plugin-check-file": "^3.3.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"nodemon": "^3.1.11",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.3.0",
		"prisma": "^6.19.0",
		"prisma-case-format": "^2.2.1",
		"source-map-support": "^0.5.21",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.5",
		"ts-loader": "^9.5.4",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.9.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"moduleNameMapper": {
			"^@common/(.*)$": "<rootDir>/src/common/$1",
			"^@modules/(.*)$": "<rootDir>/src/modules/$1",
			"^@shared/(.*)$": "<rootDir>/src/shared/$1"
		},
		"rootDir": ".",
		"testEnvironment": "node",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		}
	}
}
